<div id="landingContainer" class="container-fluid">
</div>

<div class="wrapper">
  <div id="carouselExampleIndicators" class="carousel slide" data-interval="false" data-pause="false"> <%# data-ride="carousel" %>
    <ol class="carousel-indicators">
      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>

    <div class="carousel-inner">
      <div class="carousel-item active">
          <video autoplay muted class="video" id="video1" >
            <source src="https://res.cloudinary.com/dvpavvw1u/video/upload/v1632991947/Carousel1bigtitle_cwulor.mp4" type="video/mp4">  <%# class="d-block w-100" %>
          </video>
      </div>

      <div class="carousel-item">
          <%# <video controls autoplay muted class="video" id="video2" > %>
          <video autoplay muted class="video" id="video2" >
            <source src="https://res.cloudinary.com/dvpavvw1u/video/upload/v1632874673/Carousel2_pgbx7k.mp4" type="video/mp4">
          </video>
      </div>

      <div class="carousel-item">
          <video autoplay muted class="video" id="video3" >
            <source src="https://res.cloudinary.com/dvpavvw1u/video/upload/v1632874681/Carousel3_vg7rtj.mp4" type="video/mp4">
          </video>
      </div>
    </div>

    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
  <div class="push"></div>
</div>

<script>

  $('.carousel').carousel({
    interval: 7000
  })

  $("#carouselExampleIndicators").on('slid.bs.carousel', function () {
    var vids = $(this).find(".active video");
    if(vids.length > 0){
        vids[0].pause();
        vids[0].currentTime = 0;
        vids[0].play();
    }
  })

</script>

